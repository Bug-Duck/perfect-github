<?xml version='1.0' encoding='utf-8'?>
<widget id="org.bugduck.perfectgithub" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="SecureStorage">
        <param name="android-package" value="com.crypho.plugins.SecureStorage" />
    </feature>
    <name>Perfect GitHub</name>
    <author email="dev@cordova.apache.org" href="https://cordova.apache.org">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
